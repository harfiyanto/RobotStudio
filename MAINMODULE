	CONST robtarget pPallet90Via10:=[[433.23,306.73,40.16],[0.0024531,-0.695614,0.718204,0.0172648],[0,0,-1,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
	CONST robtarget pPallet90Via20:=[[433.26,306.73,-20.32],[0.00247707,-0.6956,0.718218,0.0172488],[0,0,-1,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
	CONST robtarget pPallet3Via:=[[507.97,304.84,68.59],[0.00881255,-0.0279233,0.999556,0.00560182],[0,0,0,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
	CONST robtarget pPallet3Via10:=[[507.99,304.87,-19.58],[0.00883866,-0.0279657,0.999554,0.005633],[0,0,0,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
	CONST robtarget pPallet4Via:=[[321.86,390.09,47.64],[0.00242,-0.695518,0.718297,0.0172813],[0,0,-1,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
	CONST robtarget pPallet4Via10:=[[321.86,390.10,-21.25],[0.00243516,-0.695493,0.71832,0.0173165],[0,0,-1,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
	PROC main()
		IF diSwitch1 = 1 AND diPenStand = 1 THEN
			PickPencil;
		ELSEIF diSwitch1 = 0 AND diPenStand = 0 THEN
			returnPencil;
		ENDIF
		IF diSwitch2 = 1 AND diGripSensor = 1 THEN
			nCircleNum := 0;
			FOR i FROM 1 TO 3 DO
				CircleWObj;
				Add nCircleNum, 1;
			ENDFOR
			UIMsgBox "3 circles drawn.";
		ELSEIF FALSE AND diGripSensor = 1 THEN
            nTriangleNum := 0;
			FOR i FROM 1 TO 3 DO
				TriangleWObj;
				Add nTriangleNum, 1;
			ENDFOR
			UIMsgBox "3 triangles drawn.";
		ELSEIF diSwitch4 = 1 THEN
			WHILE diSwitch4 = 1 DO
				Palletising;
			ENDWHILE
		ENDIF
	ENDPROC
	PROC Shape()
		MoveJ pAboveStar, v200, z20, tool0;
		MoveL pAboveStar10, v200, fine, tool0;
		MoveL pAboveStar20, v200, z50, tool0;
		MoveL pAboveStar30, v200, z0, tool0;
		MoveL pAboveStar40, v200, z50, tool0;
		MoveL pAboveStar50, v200, z0, tool0;
		MoveL pAboveStar60, v200, z50, tool0;
		MoveL pAboveStar70, v200, z0, tool0;
		MoveL pAboveStar80, v200, fine, tool0;
		MoveJ pAboveStar90, v200, fine, tool0;
		MoveJ pAboveStar250, v200, fine, tool0;
		MoveL pAboveStar100, v200, z50, tool0;
		MoveL pAboveStar110, v200, z0, tool0;
		MoveL pAboveStar120, v200, z50, tool0;
		MoveL pAboveStar130, v200, z0, tool0;
		MoveL pAboveStar140, v200, z50, tool0;
		MoveL pAboveStar150, v200, z0, tool0;
		MoveL pAboveStar160, v200, z50, tool0;
		MoveL pAboveStar170, v200, z0, tool0;
		MoveL pAboveStar180, v200, z50, tool0;
		MoveL pAboveStar190, v200, z0, tool0;
		MoveL pAboveStar200, v200, z50, tool0;
		MoveL pAboveStar210, v200, z0, tool0;
		MoveL pAboveStar220, v200, z50, tool0;
		MoveL pAboveStar230, v200, z0, tool0;
		MoveL pAboveStar240, v200, fine, tool0;
		MoveJ pAboveStar260, v200, fine, tool0;
		MoveL pAboveStar270, v200, z0, tool0;
		MoveL pAboveStar280, v200, z50, tool0;
		MoveL pAboveStar290, v200, z0, tool0;
		MoveL pAboveStar300, v200, fine, tool0;
		MoveJ pAboveStar310, v200, fine, tool0;
		MoveL pAboveStar320, v200, z0, tool0;
		MoveL pAboveStar330, v200, z50, tool0;
		MoveL pAboveStar340, v200, z50, tool0;
		MoveL pAboveStar350, v200, z50, tool0;
		MoveL pAboveStar360, v200, fine, tool0;
		MoveL pAboveStar370, v200, z0, tool0;
		MoveL pAboveStar380, v200, z50, tool0;
		MoveL pAboveStar390, v200, z50, tool0;
		MoveL pAboveStar400, v200, z0, tool0;
		MoveL pAboveStar410, v200, fine, tool0;
	ENDPROC
	PROC Circle()
		MoveJ pCircle, v1000, z50, tool0;
		MoveL pCircle40, v1000, z50, tool0;
		MoveC pCircle70, pCircle80, v200, fine, tool0;
		MoveC pCircle90, pCircle100, v200, fine, tool0;
		MoveL pCircle110, v200, z50, tool0;
	ENDPROC
	PROC PickPencil()
		Set doGripper;
		MoveJ pAbovePenStand, v100, fine, toolPencil;
		MoveL pAbovePenStand20, v40, fine, toolPencil;
		Reset doGripper;
		WaitTime 0.5;
		MoveL pAbovePenStand30, v40, fine, toolPencil;
	ENDPROC
	PROC Routine1()
		MoveL [[451.82,177.96,149.37],[0.0180374,-0.0179567,0.999633,-0.0092314],[0,-1,0,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]], v1000, z50, toolPencil;
	ENDPROC
	PROC SquareWObj()
		MoveJ pWSquare, v200, z20, toolPencil\WObj:=wPaper;
		MoveL pWSquare10, v40, fine, toolPencil\WObj:=wPaper;
		MoveL pWSquare20, v40, fine, toolPencil\WObj:=wPaper;
		MoveL pWSquare30, v40, fine, toolPencil\WObj:=wPaper;
		MoveL pWSquare40, v40, fine, toolPencil\WObj:=wPaper;
		MoveL pWSquare50, v40, fine, toolPencil\WObj:=wPaper;
		MoveL pWSquare60, v40, fine, toolPencil\WObj:=wPaper;
	ENDPROC
	PROC Triangle()
		<SMT>
	ENDPROC
	PROC returnPencil()
		MoveJ pAbovePenStand, v100, fine, toolPencil;
		MoveL pAbovePenStand10, v40, fine, toolPencil;
		Set doGripper;
		WaitTime 0.5;
		MoveL pAbovePenStand, v40, fine, toolPencil;
	ENDPROC
	PROC returnHome()
		MoveJ pHome, v100, fine, toolPencil;
	ENDPROC
	PROC CircleWObj()
		!MoveJ pCircleWObj, v100, fine, toolPencil\WObj:=wPaper;
		!MoveL pCircleWObj10, v100, fine, toolPencil\WObj:=wPaper;
		!MoveC pCircleWObj20, pCircleWObj30, v100, fine, toolPencil\WObj:=wPaper;
		!MoveC pCircleWObj40, pCircleWObj50, v100, fine, toolPencil\WObj:=wPaper;
		!MoveL pCircleWObj60, v100, fine, toolPencil\WObj:=wPaper;
        MoveJ Offs(pCircleWObj,10*nCircleNum,0,0), v100, fine, toolPencil\WObj:=wPaper;
		MoveL Offs(pCircleWObj10,10*nCircleNum,0,0), v100, fine, toolPencil\WObj:=wPaper;
		MoveC Offs(pCircleWObj20,10*nCircleNum,0,0), Offs(pCircleWObj30,10*nCircleNum,0,0), v100, fine, toolPencil\WObj:=wPaper;
		MoveC Offs(pCircleWObj40,10*nCircleNum,0,0), Offs(pCircleWObj50,10*nCircleNum,0,0), v100, fine, toolPencil\WObj:=wPaper;
		MoveL Offs(pCircleWObj60,10*nCircleNum,0,0), v100, fine, toolPencil\WObj:=wPaper;
	ENDPROC
	PROC TriangleWObj()
		MoveJ Offs(pTriangleWObj,10*nTriangleNum,0,0), v100, fine, toolPencil\WObj:=wPaper;
		MoveL Offs(pTriangleWObj10,10*nTriangleNum,0,0), v40, fine, toolPencil\WObj:=wPaper;
		MoveL Offs(pTriangleWObj20,10*nTriangleNum,0,0), v40, fine, toolPencil\WObj:=wPaper;
		MoveL Offs(pTriangleWObj30,10*nTriangleNum,0,0), v40, fine, toolPencil\WObj:=wPaper;
		MoveL Offs(pTriangleWObj10,10*nTriangleNum,0,0), v40, fine, toolPencil\WObj:=wPaper;
		MoveL Offs(pTriangleWObj40,10*nTriangleNum,0,0), v100, fine, toolPencil\WObj:=wPaper;
	ENDPROC
	PROC PrintExercise()
		nMyNum := 9;
		IF nMyNum < 5 THEN
			TPWrite "My number is lower than 5 ";
		ELSEIF nMyNum < 10 THEN
			TPWrite "My number is lower than 10 ";
		ELSE
			TPWrite "My number is:"\Num:=nMyNum;
		ENDIF
	ENDPROC
	PROC offSquare()
		ClkReset cClock;
		ClkStart cClock;
		MoveJ Offs(pCircleWObj10,0,0,100), v100, fine, toolPencil\WObj:=wPaper;
		MoveL Offs(pCircleWObj10,0,0,0), v100, fine, toolPencil\WObj:=wPaper;
		MoveL Offs(pCircleWObj10,50,0,0), v100, fine, toolPencil\WObj:=wPaper;
		MoveL Offs(pCircleWObj10,50,50,0), v100, fine, toolPencil\WObj:=wPaper;
		MoveL Offs(pCircleWObj10,0,50,0), v100, fine, toolPencil\WObj:=wPaper;
		MoveL Offs(pCircleWObj10,0,0,0), v100, fine, toolPencil\WObj:=wPaper;
		MoveL Offs(pCircleWObj10,0,0,100), v100, fine, toolPencil\WObj:=wPaper;
		ClkStop clock1;
		nClock := ClkRead(cClock);
		strClock := ValToStr(nClock);
		UIMsgBox strClock;
	ENDPROC
	PROC relSquare()
		MoveJ RelTool(pCircleWObj10,0,0,-100), v100, fine, toolPencil\WObj:=wPaper;
		MoveJ RelTool(pCircleWObj10,0,0,-100\Rx:=30\Rz:=30), v100, fine, toolPencil\WObj:=wPaper;
		MoveL RelTool(pCircleWObj10,0,0,0\Rx:=30\Rz:=30), v100, fine, toolPencil\WObj:=wPaper;
		MoveL RelTool(pCircleWObj10,50,0,0\Rx:=30\Rz:=30), v100, fine, toolPencil\WObj:=wPaper;
		MoveL RelTool(pCircleWObj10,50,50,0\Rx:=30\Rz:=30), v100, fine, toolPencil\WObj:=wPaper;
		MoveL RelTool(pCircleWObj10,0,50,0\Rx:=30\Rz:=30), v100, fine, toolPencil\WObj:=wPaper;
		MoveL RelTool(pCircleWObj10,0,0,0\Rx:=30\Rz:=30), v100, fine, toolPencil\WObj:=wPaper;
		MoveL RelTool(pCircleWObj10,0,0,-100), v100, fine, toolPencil\WObj:=wPaper;
	ENDPROC
	PROC PickPlace()
		ClkReset cClock;
		ClkStart cClock;
		Set doGripper;
		MoveJ pPickVia, v100, z50, toolPencil;
		MoveJ pPickVia20, v100, z50, toolPencil;
		MoveL pPickVia30, v100, fine, toolPencil;
		Reset doGripper;
		Set doMagnet;
		WaitTime 1;
		MoveL pPickVia30, v100, z50, toolPencil;
		MoveJ pPickVia20, v100, z50, toolPencil;
		MoveJ pPickVia, v100, z50, toolPencil;
		MoveJ pPickVia40, v100, z50, toolPencil;
		MoveL pPickVia50, v100, fine, toolPencil;
		Set doGripper;
		Reset doMagnet;
		WaitTime 1;
		MoveL pPickVia50, v100, fine, toolPencil;
		MoveJ pPickVia40, v100, z50, toolPencil;
		ClkStop cClock;
		TPWrite "Cycle time: "\Num:=ClkRead(cClock);
	ENDPROC
	PROC PickPlaceFast()
		ClkReset cClock;
		ClkStart cClock;
		Set doGripper;
		MoveJ [[360.73,31.38,132.06],[0.0090413,-0.0279876,0.999552,0.00561662],[0,0,0,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]], v100, z50, toolPencil;
		MoveJ pPickVia20, v100, z50, toolPencil;
		MoveL pPickVia30, v100, fine, toolPencil;
		Reset doGripper;
		Set doMagnet;
		WaitTime 1;
		MoveL pPickVia30, v100, z50, toolPencil;
		MoveJ pPickVia20, v100, z50, toolPencil;
		MoveJ pPickVia, v100, z50, toolPencil;
		MoveJ pPickVia40, v100, z50, toolPencil;
		MoveL pPickVia50, v100, fine, toolPencil;
		Set doGripper;
		Reset doMagnet;
		WaitTime 1;
		MoveL pPickVia50, v100, fine, toolPencil;
		MoveJ pPickVia40, v100, z50, toolPencil;
		ClkStop cClock;
		TPWrite "Cycle time: "\Num:=ClkRead(cClock);
	ENDPROC
	PROC RevCounter()
		MoveAbsJ [[0,0,0,0,0,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]]\NoEOffs, v100, z50, toolPencil;
	ENDPROC
	PROC Palletising()
		MoveJ pPalletVia, v1000, z200, toolPencil;
		WaitDI diSwitch3, 0;
		MoveJ pPalletVia10, v1000, z200, toolPencil;
		MoveL pPalletVia20, v1000, fine, toolPencil;
		Set doMagnet;
		WaitTime 0.25;
		MoveL pPalletVia10, v1000, z200, toolPencil;
		MoveJ pPalletVia, v1000, z200, toolPencil;
		MoveJ pPalletVia30, v1000, z200, toolPencil;
		MoveL pPalletVia40, v1000, fine, toolPencil;
		Reset doMagnet;
		WaitTime 0.25;
		MoveL pPalletVia30, v1000, z50, toolPencil;
		!2
		MoveJ pPalletVia, v1000, z200, toolPencil;
		WaitDI diSwitch3, 1;
		MoveJ pPalletVia10, v1000, z200, toolPencil;
		MoveL pPalletVia20, v1000, fine, toolPencil;
		Set doMagnet;
		WaitTime 0.25;
		MoveL pPalletVia10, v1000, z200, toolPencil;
		MoveJ pPallet90Via, v1000, z200, toolPencil;
		MoveJ pPallet90Via10, v1000, z200, toolPencil;
		MoveL pPallet90Via20, v1000, fine, toolPencil;
		Reset doMagnet;
		WaitTime 0.25;
		MoveL pPallet90Via10, v1000, z200, toolPencil;
		!3
		MoveJ pPalletVia, v1000, z200, toolPencil;
		WaitDI diSwitch3, 0;
        MoveJ pPalletVia10, v1000, z200, toolPencil;
		MoveL pPalletVia20, v1000, fine, toolPencil;
		Set doMagnet;
		WaitTime 0.25;
		MoveL pPalletVia10, v1000, z200, toolPencil;
		MoveJ pPalletVia, v1000, z200, toolPencil;
		MoveJ pPallet3Via, v1000, z200, toolPencil;
		MoveL pPallet3Via10, v1000, fine, toolPencil;
		Reset doMagnet;
		WaitTime 0.25;
		MoveL pPallet3Via, v1000, z50, toolPencil;
        !4
        MoveJ pPalletVia, v1000, z200, toolPencil;
		WaitDI diSwitch3, 1;
		MoveJ pPalletVia10, v1000, z200, toolPencil;
		MoveL pPalletVia20, v1000, fine, toolPencil;
		Set doMagnet;
		WaitTime 0.25;
		MoveL pPalletVia10, v1000, z200, toolPencil;
		MoveJ pPallet90Via, v1000, z200, toolPencil;
		MoveJ pPallet4Via, v1000, z200, toolPencil;
		MoveL pPallet4Via10, v1000, fine, toolPencil;
		Reset doMagnet;
		WaitTime 0.25;
		MoveL pPallet4Via, v1000, z200, toolPencil;
	ENDPROC
ENDMODULE
